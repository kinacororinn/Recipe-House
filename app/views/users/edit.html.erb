<h1>マイページ編集</h1>
<!-- form_with部分 -->
<%= form_with model: @user,local: true do |f| %>

  <h4>名前</h4>
   <%= f.text_field :name %>
  <h4>画像</h4>
   <%= f.attachment_field :profile_image %>
  <h4>紹介文</h4>
  <%= f.text_field :introduction %>
  <%= f.submit '変更を保存' %>
  <%#= カテゴリー作るまでコメントアウト　%>
  <%#= f.label :ジャンル %>
  <%#= form.select :category.genre, ['煮物', '料理', 'おやつ'], {include_blank: '---'} %>
<% end %>

